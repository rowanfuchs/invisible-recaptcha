{
    "name": "rowanfuchs/invisible-recaptcha",
    "version": "1.0.1",
    "description": "Invisible reCAPTCHA For Laravel.",
    "keywords": [
        "recaptcha",
        "no-captcha",
        "captcha",
        "invisible",
        "invisible-recaptcha",
        "laravel",
        "laravel5",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Chen",
            "homepage": "https://www.albert-chen.com"
        },
        {
            "name": "Rowan Fuchs",
            "homepage": "https://rowanfuchs.nl"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.3",
        "illuminate/support": "^9.0|^11.0",
        "illuminate/view": "^9.0|^11.0",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "AlbertCht\\InvisibleReCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlbertCht\\InvisibleReCaptcha\\InvisibleReCaptchaServiceProvider"
            ]
        }
    }
}
